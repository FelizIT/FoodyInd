@model IList<ProductOverviewModel>

@using Nop.Web.Models.Catalog;

 @if (Model.Count > 0)
        {
            <div class="product-list" >
                <div class="item-grid">
                    @foreach (var product in Model)
                    {
                        <div class="item-box">
                            @Html.Partial("_ProductBox", product)
                        </div>
                    }
                </div>
            </div>
        }
       